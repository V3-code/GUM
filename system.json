  "authors": [
    {
      "name": "Victor Valentim",
      "url": "https://github.com/V3-code"
    }
  ],

  "compatibility": {
    "minimum": "11",
    "verified": "13"
  },
  "esmodules": [
    "scripts/gum-data.js",
    "scripts/apps/condition-builder.js",
    "scripts/apps/effect-builder.js",
    "scripts/apps/effect-sheet.js",
    "scripts/apps/condition-sheet.js",
    "scripts/apps/damage-application.js",
    "module/settings.js",
    "module/apps/modifier-browser.js",
    "module/apps/effect-browser.js",
    "scripts/main.js"
  ],
  "styles": [
    "styles/styles.css",
    "styles/modifier-browser.css",
    "styles/effect-browser.css",
    "styles/item-sheet.css",
    "styles/damage-application.css",
    "styles/conditions.css",
    "styles/condition-builder.css" 
